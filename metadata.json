{
  "name": "fnerdwq-omd",
  "version": "0.0.1",
  "author": "Frederik Wagner",
  "summary": "This module install the Open Monitoring Distribution (Check_MK / Nagios)",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/fnerdwq/puppet-omd.git",
  "project_page": "https://github.com/fnerdwq/puppet-omd",
  "issues_url": "https://github.com/fnerdwq/puppet-omd/issues",
  "description": "Modul installing the Open Monitoring Distribution OMD",
  "tags": [
    "nagios",
    "omd",
    "check_mk",
    "check-mk",
    "monitoring"
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.6.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.1.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.5.0"
    },
    {
      "name": "stahnma/epel",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "nanliu/staging",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs/xinetd",
      "version_requirement": ">= 1.3.1"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.1"
    },
    {
      "name": "dalen/puppetdbquery",
      "version_requirement": ">= 1.5.2"
    }
  ]
}
